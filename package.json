{
  "name": "psbapi-search-tracker",
  "version": "1.0.1",
  "private": true,
  "description": "Search tracker microservice",
  "type": "module",
  "scripts": {
    "wait-for-mysql": "wait-on \"tcp:$MYSQL_HOST:3306\" -t 180000",
    "build": "tsc",
    "postbuild": "copyfiles -e '**/*.mts' -e '**/*.ts' 'src/**/*' 'test/**/*' dist/",
    "start:dev": "nodemon",
    "lint": "eslint -f gha .",
    "lint:fix": "eslint --fix .",
    "pretest": "npm run lint",
    "test": "mocha",
    "posttest": "npm run build",
    "pretest:coverage": "rimraf dist; npm run build",
    "test:coverage": "cross-env JS_TESTS=1 c8 mocha dist/test"
  },
