{
  "name": "psbapi-search-tracker",
  "version": "0.0.1",
  "private": true,
  "description": "Search tracker microservice",
  "type": "module",
  "scripts": {
    "wait-for-mysql": "wait-on \"tcp:$MYSQL_HOST:3306\" -t 180000",
    "build": "tsc",
    "start:dev": "nodemon",
    "lint": "eslint -f gha .",
    "lint:fix": "eslint --fix .",
    "pretest": "npm run lint",
    "test": "mocha",
    "test:coverage": "c8 mocha",
    "posttest": "npm run build",
    "prescan": "npm run wait-for-mysql && ts-node ./test/migrate.mts",
    "scan": "npm run start:dev"
  },
