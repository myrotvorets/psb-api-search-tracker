    "envalid": "^8.1.0",
    "express": "^5.1.0",
    "express-openapi-validator": "^5.5.8",
    "inet_xtoy": "^1.2.7",
    "knex": "^3.1.0",
    "morgan": "^1.10.1",
    "mysql2": "^3.14.4"
  },
  "devDependencies": {
    "@myrotvorets/eslint-config-myrotvorets-ts": "^3.0.2",
    "@types/chai": "^5.2.2",
