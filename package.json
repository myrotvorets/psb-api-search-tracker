    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.9.1"
  },
  "devDependencies": {
    "@myrotvorets/eslint-config-myrotvorets-ts": "^2.25.0",
    "@types/chai": "^4.3.11",
    "@types/express": "^4.17.21",
    "@types/ip": "^1.1.3",
    "@types/mocha": "^10.0.6",
    "@types/mock-knex": "^0.4.8",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.19",
    "@types/supertest": "^6.0.2",
    "c8": "^9.1.0",
    "chai": "^5.1.0",
    "eslint-formatter-gha": "^1.4.3",
